{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "command",
  "coverageAnalysis": "off",
  "checkers": [
    "typescript"
  ],
  "tsconfigFile": "tsconfig.json",
  "mutate": [
    "src/**/*.ts",
    "!src/putting_it_together/**/*.ts",
    "!src/cucumber.ts"
  ],
  "timeoutMS": 10000,
  "mutator": {
    "excludedMutations": [
      "StringLiteral"
    ]
  },
  "jest": {
    "projectType": "custom",
    "configFile": "jest.config.js"
  }
}